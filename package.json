{
  "name": "@zimo-elektronik/zcan",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/ZIMO-Elektronik/zcan",
  "version": "1.0.13",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nodemon --ext js,ts,json,env --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm\" src/main.ts",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "jest --forceExit",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "test:selected": "jest --forceExit --runTestsByPath D:\\Projects\\zcan\\__tests__\\accessoriesGroup.test.ts",
    "lint": "eslint --ext=.ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check",
    "docs": "gulp documentation"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cross-env": "^7.0.3",
    "del": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "gulp": "^5.0.0",
    "gulp-spawn": "^2.0.0",
    "gulp-typedoc": "git+https://github.com/crutchcorn/gulp-typedoc.git",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.8",
    "typescript": "5.5.4"
  }
}
